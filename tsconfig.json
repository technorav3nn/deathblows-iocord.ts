{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2021",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "outDir": "dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2017", "esnext.asynciterable", "dom"],
    "moduleResolution": "node"
  },
  "exclude": ["node_modules"]
}
